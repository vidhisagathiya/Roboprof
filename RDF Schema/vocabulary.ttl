@prefix cu: <http://is-concordia.io/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/spec/>

# Classes
cu:University
    a               rdfs:Class ;
    rdfs:subClassOf foaf:Organization ;
    rdfs:label      "University" .
    rdfs:comment "Represents a University"@en .

cu:Course
    a               rdfs:Class ;
    rdfs:label      "Course" .
    rdfs:comment "Represents a Course"@en .

cu:Lecture
    a               rdfs:Class ;
    rdfs:label      "Lecture" .
    rdfs:comment "Represents a Lecture"@en .

cu:LectureContent 
    a               rdfs:Class ;
    rdfs:label "Lecture Content" ;
    rdfs:comment "Represents Lecture Content"@en .

cu:Worksheet
    a               rdfs:Class ;
    rdfs:subClassOf cu:LectureContent
    rdfs:label      "Worksheet" .
    rdfs:comment "Represents a Worksheet"@en .

cu:Slide
    a               rdfs:Class ;
    rdfs:subClassOf cu:LectureContent
    rdfs:label      "Slide" .
    rdfs:comment "Represents a Slide"@en .

cu:Topic
    a               rdfs:Class ;
    rdfs:label      "Topic" .
    rdfs:comment "Represents a Topic"@en .

cu:Student
    a               rdfs:Class ;
    rdfs:subClassOf foaf:Person ;
    rdfs:label "Student";
    rdfs:comment "Represents a Student"@en .



# Properties
cu:Offers
    a            rdf:Property ;
    rdfs:label   "Offers" ;
    rdfs:comment "Specifies the course offered by the University"@en ;
    rdfs:domain  cu:University ;
    rdfs:range   rdfs:Literal .

cu:hasCourseSubject
    a            rdf:Property ;
    rdfs:label   "hasCourseSubject" ;
    rdfs:comment "Specifies the subject of a course"@en ;
    rdfs:domain  cu:Course ;
    rdfs:range   rdfs:Literal .

cu:hasCourseNumber
    a            rdf:Property ;
    rdfs:label   "hasCourseNumber" ;
    rdfs:comment "Specifies the number of a course"@en ;
    rdfs:domain  cu:Course ;
    rdfs:range   rdfs:Literal .

cu:hasCourseDescription
    a            rdf:Property ;
    rdfs:label   "hasCourseDescription" ;
    rdfs:comment "Specifies the description of a course"@en ;
    rdfs:domain  cu:Course ;
    rdfs:range   rdfs:Literal .

cu:hasCourseOutline
    a            rdf:Property ;
    rdfs:label   "hasCourseOutline" ;
    rdfs:comment "Specifies the outline of a course"@en ;
    rdfs:domain  cu:Course ;
    rdfs:range   rdfs:Literal .

cu:hasCredits
    a            rdf:Property ;
    rdfs:label   "hasCredits" ;
    rdfs:comment "Specifies the number of credits of a course"@en ;
    rdfs:domain  cu:Course ;
    rdfs:range   rdfs:Literal .    

cu:hasLecture
    a            rdf:Property ;
    rdfs:label   "hasLecture" ;
    rdfs:comment "Specifies the Lecture"@en ;
    rdfs:domain  cu:Course ;
    rdfs:range   rdfs:Literal .

cu:hasLectureNumber
    a            rdf:Property ;
    rdfs:label   "hasLectureNumber" ;
    rdfs:comment "Specifies the number of a lecture"@en ;
    rdfs:domain  cu:Lecture ;
    rdfs:range   rdfs:Literal .

cu:hasLectureContent
    a            rdf:Property ;
    rdfs:label   "hasLectureContent" ;
    rdfs:comment "Specifies the content of a lecture"@en ;
    rdfs:domain  cu:Lecture ;
    rdfs:range   rdfs:LectureContent .

cu:topicsCovered
    a            rdf:Property ;
    rdfs:label   "Topics covered in a Lecture" ;
    rdfs:comment "Specifies the Topics covered in a lecture"@en ;
    rdfs:domain  cu:Lecture ;
    rdfs:range   rdfs:Topic .

cu:hasTopic
    a            rdf:Property ;
    rdfs:label   "hasTopic" ;
    rdfs:comment "Specifies the topic" ;
    rdfs:domain  cu:Topic ;
    rdfs:range   rdfs:Literal .

cu:hasProvenance
    a            rdf:Property ;
    rdfs:label   "hasProvenance" ;
    rdfs:comment "Specifies where the topic was identified as being covered in the course" ;
    rdfs:domain  cu:Topic ;
    rdfs:range   rdfs:Literal .

cu:hasMaterials
    a            rdf:Property ;
    rdfs:label   "hasMaterials" ;
    rdfs:comment "Specifies materials for the topic" ;
    rdfs:domain  cu:Topic ;
    rdfs:range   rdfs:LectureContent .

cu:hasStudentID
    a            rdf:Property ;
    rdfs:label   "hasStudentID" ;
    rdfs:comment "Specifies the ID number of a student" ;
    rdfs:domain  cu:Student ;
    rdfs:range   rdfs:Literal .

cu:hasStudentEmail
    a            rdf:Property ;
    rdfs:label   "hasStudentEmail" ;
    rdfs:comment "Specifies the email address of a student" ;
    rdfs:domain  cu:Student ;
    rdfs:range   rdfs:Literal .

cu:isEnrolledIn
    a            rdf:Property ;
    rdfs:label   "isEnrolledIn" ;
    rdfs:comment "Specifies the courses of a student" ;
    rdfs:domain  cu:Student ;
    rdfs:range   rdfs:Course .

cu:hasCompetency
    a            rdf:Property ;
    rdfs:label   "hasCompetency" ;
    rdfs:comment "Specifies the competencies of a student" ;
    rdfs:domain  cu:Student ;
    rdfs:range   rdfs:Topic .